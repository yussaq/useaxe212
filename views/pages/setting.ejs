<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../patrials/head'); %>
</head>
<body>
    <header>
        <%- include('../patrials/header'); %>
    </header>
    <main>
        <%- include('../patrials/title'); %>        
        <div class="container">
        <form>
            <div class="row mb-3">
                <label for="collection_name" class="col-sm-2 col-form-label">Collections Name</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="collection_name" id="collection_name">
                </div>
            </div>
            <div class="row mb-3">
                <label for="collection_description" class="col-sm-2 col-form-label">Description</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="collection_description" id="collection_description">
                </div>
            </div>
            <div class="row mb-3">
                <label for="collection_ipfs" class="col-sm-2 col-form-label">IPFS</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="collection_ipfs" id="collection_ipfs">
                </div>
            </div>
            <div class="row mb-3">
                <label for="collection_attributes" class="col-sm-2 col-form-label">IPFS</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="collection_attributes" id="collection_attributes">
                </div>
            </div>            
            <div class="row mb-3">
                <label for="collection_creator" class="col-sm-2 col-form-label">Creator</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" name="collection_creator" id="collection_creator">
                </div>
            </div>           
            <div class="row mb-3">
                <label for="collection_creator" class="col-sm-2 col-form-label">&nbsp;</label>
                <div class="col-sm-10">
                    <button type="submit" class="btn btn-primary">Save</button>
                </div>
            </div>           
          </form>
          </div>
    </main>
    <footer>
        <%- include('../patrials/footer'); %>
    </footer>
</body>
</html>
